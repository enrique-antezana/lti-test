{
  "functions": {
    "api/app.py": {
      "runtime": "@vercel/python@4.3.0",
      "includeFiles": "api/**/*"
    }
  },
  "routes": [
    {
      "src": "/(.*\\.(png|jpg|jpeg|gif|css|js|ico|txt|pdf|html))",
      "dest": "/templates/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/app.py"
    }
  ]
}
